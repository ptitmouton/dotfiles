{
  "name": "coc-graphql",
  "version": "0.0.1",
  "description": "GraphQL Language Service extension for coc.nvim",
  "main": "lib/index.js",
  "repository": "https://github.com/felippepuhle/coc-graphql",
  "author": "felippepuhle",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -rf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prettier": "prettier --write '(server|src)/**/*.ts'"
  },
  "engines": {
    "coc": "^0.0.77"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "GraphQL",
      "properties": {
        "graphql.debug": {
          "type": "boolean",
          "default": false
        },
        "graphql.filetypes": {
          "type": "array",
          "default": [
            "graphql",
            "javascript",
            "javascriptreact",
            "typescript",
            "typescriptreact",
            "reason"
          ],
          "items": {
            "type": "string"
          }
        },
        "graphql.watcherPattern": {
          "type": "string",
          "default": "**/*.{graphql,gql,js,jsx,ts,tsx,re}"
        }
      }
    }
  },
  "devDependencies": {
    "coc.nvim": "^0.0.77",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "graphql": "^15.3.0",
    "graphql-language-service-server": "^2.4.0",
    "prettier": "^2.0.5"
  }
}
